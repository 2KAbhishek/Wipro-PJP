<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wipro.bean.Emp" table="emp">
		<id name="no" column="empno" type="int">
			<generator class="assigned"></generator>
		</id>
		<property name="name" column="ename" type="string"></property>
		<property name="job" column="job" type="string"></property>
		<property name="manager" column="mgr" type="int"></property>
		<property name="hireDate" column="hiredate" type="date"></property>
		<property name="salary" column="sal" type="double"></property>
		<property name="commission" column="comm" type="double"></property>
		<property name="deptNo" column="deptno" type="int"></property>
	</class>
</hibernate-mapping>